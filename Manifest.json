{
    
    "name": "Sol Searching",
    "version": "2.0.0",
    "manifest_version": 2,
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["Content.js"]
        }
    ],
 
    
    "permissions": [
        "tabs"
        ],
    
    "browser_action": {
        "default_popup": "Popup.html",
        "default_title": "Sol Searching",
        "default_icon": "logo.png"
    },
    
    "background":{
    "scripts": ["background.js"],
    "persistent": false
}
    
}
